{
  "name": "nextjs-starter",
  "version": "4.1.0",
  "main": "index.js",
  "repository": "",
  "license": "MIT",
  "volta": {
    "node": "18.14.2",
    "yarn": "1.22.19"
  },
  "browserslist": [
    "> 0.2%, not dead",
    "not IE 11",
    "Safari > 14, iOS > 14"
  ],
  "scripts": {
    "prepare": "rimraf .config && husky install .config/husky && husky add .config/husky/pre-commit \"yarn lint-staged\"",
    "dev": "node server",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true yarn start",
    "export": "next build && next export && next-export-optimize-images && next-sitemap --config sitemap.config.js",
    "start": "next build && next start -p 8888",
    "clean": "rimraf .next out",
    "lint": "yarn fix:eslint && yarn fix:stylelint && yarn prettier",
    "fix:stylelint": "stylelint --fix '**/*.scss' && prettier --write '**/*.scss'",
    "fix:eslint": "eslint --fix --ext .ts,.tsx --ignore-pattern .eslintignore . && prettier --write '**/*.{ts,tsx}'",
    "prettier": "prettier --write '**/*.{js,json,css}'",
    "svgr": "yarn svgr:icon",
    "svgr:icon": "svgo -f src/components/ui/icon/assets --config ./svgo.config.js && svgr --filename-case kebab src/components/ui/icon/assets/*.svg --ext tsx --out-dir src/components/ui/icon",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "generate": "sg wizard"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.2.1",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/addon-viewport": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@svgr/cli": "6.5.1",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-transition-group": "4.4.5",
    "@types/recoil": "0.0.9",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "autoprefixer": "10.4.13",
    "browserslist": "4.21.5",
    "chalk": "4.1.2",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "css-mqpacker": "7.0.0",
    "cssnano": "5.1.15",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "glslify-loader": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "next-export-optimize-images": "2.0.0",
    "next-sitemap": "3.1.52",
    "next-transpile-modules": "10.0.0",
    "postcss": "8.4.21",
    "postcss-calc": "8.2.4",
    "postcss-discard-duplicates": "5.1.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "7.0.2",
    "postcss-preset-env": "8.0.1",
    "postcss-sorting": "8.0.1",
    "prettier": "2.8.4",
    "raw-loader": "4.0.2",
    "rimraf": "4.1.2",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "seng-generator": "0.9.0",
    "stylelint": "15.2.0",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-config-recommended-scss": "9.0.1",
    "stylelint-config-standard": "30.0.1",
    "stylelint-config-standard-scss": "7.0.1",
    "stylelint-declaration-block-no-ignored-properties": "2.7.0",
    "stylelint-order": "6.0.2",
    "stylelint-scss": "4.4.0",
    "svgo": "3.0.2",
    "typescript": "4.9.5",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@next/font": "13.2.3",
    "@welldone-software/why-did-you-render": "7.0.1",
