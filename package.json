    "eventemitter3": "5.0.0",
    "gsap": "3.11.4",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-transition-group": "4.4.5",
    "recoil": "0.7.6",
    "ress": "5.0.2",
    "ua-parser-js": "1.0.33"
  }
